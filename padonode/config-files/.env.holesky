AO_TASKS_PROCESS_ID='fX6ek4jPhcpM0XIOw2CNO2xiOUYGPuaRHXztW9_JWsY'

#
# ARWEAVE API CONFIG
#
ARWEAVE_API_CONFIG_HOST='arweave.net'
ARWEAVE_API_CONFIG_PORT=443
ARWEAVE_API_CONFIG_PROTOCOL='https'

#
# storage type: ARSEEDING_ETH, ARSEEDING_AR, ARWEAVE
#
DATA_STORAGE_TYPE='ARSEEDING_ETH'

#
# Enables
#
ENABLE_EIGEN_LAYER=true
ENABLE_AO=false
