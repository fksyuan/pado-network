#
# PADO Management Contracts
#
DATA_MANAGEMENT_ADDRESS=0x41Cd32758A3fFc04565047D4AF1248DE455c705F
TASK_MANAGEMENT_ADDRESS=0x00430BF32B6799E58C48C6a5be5DF9fef7D314ED
